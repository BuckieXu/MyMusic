.bgimg{
  width: 100%;
  height: 100%;
  z-index: -100;
  position: fixed;
}
.listItem{
  width: 100%;
  padding-bottom: 60rpx;
}
.titletab{
  display: flex;
  flex-wrap: wrap;
}
.title{
  height: 60rpx;
  line-height: 60rpx;
  color: white;
  padding-left: 40rpx;
}
.tab{
  height: 60rpx;
  width: 150rpx;
  margin-left: 30rpx;
  margin-bottom: 16rpx;
  border-radius: 30rpx;
  border: 1rpx solid #43847c;
  line-height: 60rpx;
  text-align: center;
  color: white;
  font-size: 26rpx;
  background-color: #43847c;
  opacity: 0.9;
}